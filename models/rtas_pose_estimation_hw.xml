<xml>

<!-- system //-->
<system name="pose_estimation_hw"> 
	<child name="CAM" >
		<function name="CAMERA" />
		<dependency>
			<child name="I2C" />
			<child name="GPIO" />
			<child name="VDMA" />
			<child name="PL_IRQ" />
			<child name="TRIGGER" />
			<function name="TIMER" />
		</dependency>
	</child>

	<child name="STEREO_PREPROC" >
		<function name="STEREO_PREPROC" />
		<dependency>
            <child name="CAM" />
            <child name="VDMA" />
            <child name="PL_IRQ" />
            <child name="GPIO" />
			<child name="Dpr" />
			<function name="TIMER" />
        </dependency>
	</child>

	<child name="Dpr" >
		<function name="Dpr" />
		<dependency>
			<child name="FILE_SYSTEM" />
			<function name="TIMER" />
		</dependency>
	</child>

	<child name="FILE_SYSTEM" >
		<function name="FILE_SYSTEM" />
		<dependency> 
			<child name="SD_CARD" />
		</dependency>
	</child>

	<child name="I2C" ><function name="I2C" /></child>
	<child name="GPIO" ><function name="GPIO" /></child>
	<child name="VDMA" ><function name="VDMA" />
		<dependency>
			<function name="TIMER" />
		</dependency>
	</child>
	<child name="PL_IRQ" ><function name="PL_IRQ" />
		<dependency>
			<function name="TIMER" />
		</dependency>
	</child>
	<child name="SD_CARD" ><function name="SD_CARD" /></child>

	<child>
		<function name="POINTCLOUD"/>
		<dependency>
			<function name="STEREO_PREPROC_LEFT" />
			<function name="STEREO_PREPROC_RIGHT" />
		</dependency>
	</child>
	
	<child name="POSE" subsystem="pc" >
		<component name="pose_estimation" />
		<dependency>
			<function name="POINTCLOUD" />
		</dependency>
	</child>

	<child name="TRIGGER" subsystem="pc">
	    <function name="TRIGGER" />
		<dependency>
			<function name="TIMER" />
		</dependency>
	</child>

</system>
</xml>
